{
  "class": "CommandLineTool",
  "name": "create-assay",
  "description": "Create formatted toxicity assay files",
  "baseCommand": ["/bin/bash", "-c"],
  "arguments": [
    "python src/data/raw_to_assays.py $(inputs.unformatted_data.filepath) /outputs --dataset $(inputs.dataset.default)"
  ],
  "dockerPull": "sethhowes/fs-tox@sha256:715516b10859255180bb586be9935ab407a027ce3d574fbbe9d9d328f7c29cf8",
  "gpuBool": false,
  "networkBool": false,
  "inputs": {
    "unformatted_data": {
      "type": "File",
      "glob": ["*.csv"]
    },
    "dataset": {
      "type": "string",
      "default": "tox21"
    }
  },
  "outputs": {
    "formatted_assays": {
      "type": "Array",
      "item": "File",
      "glob": ["*.parquet"]
    }
  }
}
